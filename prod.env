# 生产环境配置
ENVIRONMENT=prod
DEBUG=false
LOG_LEVEL=INFO

APP_NAME=PowerCapFastAPI
APP_VERSION=1.0.0
API_V1_PREFIX=/api/v1
API_WORKERS=2
ALLOWED_HOSTS=["your.domain.com"]

SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=1440

CELERY_TIMEZONE=Asia/Shanghai
CELERY_CONCURRENCY=4
CELERY_TASK_SOFT_TIME_LIMIT=3600
CELERY_TASK_TIME_LIMIT=7200

PROD_REDIS_CLUSTER_PASSWORD=your_password
PROD_REDIS_CLUSTER_DECODE_RESPONSES=true
PROD_REDIS_CLUSTER_SOCKET_TIMEOUT=5
PROD_REDIS_CLUSTER_SOCKET_CONNECT_TIMEOUT=5
PROD_REDIS_CLUSTER_SKIP_FULL_COVERAGE_CHECK=true
PROD_REDIS_CLUSTER_NODES=[{"host":"redis-node1","port":6379},{"host":"redis-node2","port":6379},{"host":"redis-node3","port":6379}] 